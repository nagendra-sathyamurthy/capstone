                    "",
                    "pm.test('Response contains legacy items array', function () {",
                    "    const response = pm.response.json();",
                    "    pm.expect(response).to.be.an('array');",
                    "});",
                    "",
                    "pm.test('Legacy items have required properties', function () {",
                    "    const response = pm.response.json();",
                    "    if (response.length > 0) {",
                    "        const firstItem = response[0];",
                    "        pm.expect(firstItem).to.have.property('Id');",
                    "        pm.expect(firstItem).to.have.property('Name');",
                    "        pm.expect(firstItem).to.have.property('Price');",
                    "        pm.globals.set('legacy_item_id', firstItem.Id);",
                    "    }",
                    "});",
                    "",
                    "pm.test('Legacy format maintains backward compatibility', function () {",
                    "    const response = pm.response.json();",
                    "    response.forEach(item => {",
                    "        // Check for PascalCase properties (legacy format)",
                    "        pm.expect(item).to.have.property('Id');",
                    "        pm.expect(item).to.have.property('Name');",
                    "        pm.expect(item).to.have.property('Price');",
                    "        if (item.Description) {",
                    "            pm.expect(item).to.have.property('Description');",
                    "        }",
